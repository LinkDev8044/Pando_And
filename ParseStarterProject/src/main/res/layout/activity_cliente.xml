<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VistaClientes.ClienteActivity">

    <FrameLayout
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/textView14"
        >

    </FrameLayout>

    <android.support.design.widget.BottomNavigationView
        android:id="@+id/bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:menu="@menu/bottom_navigation"

        ></android.support.design.widget.BottomNavigationView>

    <TextView
        android:id="@+id/textView10"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_above="@+id/bottom_navigation"
        android:background="#cccccc"
        android:text="TextView" />
    <TextView
        android:id="@+id/textView11"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_above="@+id/textView10"
        android:background="#d6d6d6"
        android:text="TextView" />
    <TextView
        android:id="@+id/op1ContratextView"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_above="@+id/textView11"
        android:background="#e0e0e0"
        android:text="TextView" />
    <TextView
        android:id="@+id/textView13"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_above="@+id/op1ContratextView"
        android:background="#ebebeb"
        android:text="TextView" />
    <TextView
        android:id="@+id/textView14"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_above="@+id/textView13"
        android:background="#f5f5f5"
        android:text="TextView" />

</RelativeLayout>