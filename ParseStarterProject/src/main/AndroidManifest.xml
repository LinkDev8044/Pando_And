<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.parse.starter">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".StarterApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo_pando_app"
        android:label="Pando"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <meta-data
            android:name="com.parse.APPLICATION_ID"
            android:value="@string/parse_app_id" />
        <meta-data
            android:name="com.parse.CLIENT_KEY"
            android:value="@string/parse_client_key" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".Inicio.inicio_Pando_Activity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".registro_Usuario.NombreUserActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".registro_Usuario.SexoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".registro_Usuario.CorreoUserActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".registro_Usuario.ContrasenaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".registro_Usuario.InfoInicialActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".VistaClientes.ClienteActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Inicio.LogInActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".VistaClientes.DescripcionComercio.DescripcionComercioActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AyudaYSugerencias.AyudaSugerenciaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".VistaClientes.TiendaComercio.TiendaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".VistaClientes.TiendaComercio.CompraTiendaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".VistaClientes.MisCompras.MisComprasActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".VistaClientes.HistorialPuntos.HistorialPuntosActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".VistaClientes.Encuestas.PreguntaCaritasActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".VistaClientes.Encuestas.PreguntaSliderActivity"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>